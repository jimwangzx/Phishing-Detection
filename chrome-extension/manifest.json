{
  "manifest_version": 2,

  "name": "QMIND Phishing Detection",
  "description": "Find any malicious/phishing links before you click them",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons":{
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background":{
    "scripts":["background.js"]
  },
  "permissions": [
   "tabs",
   "<all_urls>",
   "storage"
   ]
}